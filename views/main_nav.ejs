        width: 100%;
        padding: 20px;
        text-align: center;
        vertical-align: middle;

    }

    .transpo {
        background: transparent;
        position:absolute;
        top: 0;
    }

    #form{
      width:300px;
      margin:10px 0;
      background-color: #DFECF0;
      background-color:rgba(223, 236, 240, 0.5);
      margin-top: 20px;
      padding-top: 20px;
      padding-bottom: 20px;

    }

    .join_btn {
        width: 100%;
        margin: 0 auto;
    }

    .h_desc {
        font-size: 20pt;
        color: white;
        font-family: courier new;
        font-weight: bold;

    }

    .h_join {
        width: 75%;
        margin: 0 auto;
        border: 2pt solid white;
        font-size: 15pt;
        color: white;
        font-family: courier new;
        padding: 5px;
        font-weight:bold;
        transition: 0.3s;
    }

    .h_join:hover {
        background: white;
        color: black;
        text-decoration: none;
    }

    .logo_img {
        width: 15%;
        filter: invert(100%);
    }

    .logo_img_2nd {
        filter: invert(0%);
        width: 15%;

    }

