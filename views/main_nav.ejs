  }

  .each_col{
    margin-top: 8px;
    margin-left: 2px;
    position: relative;
  }



   .index_pic {
     width:330px;
     height:280px;
     object-fit: cover;
   }


   .up{
     position:absolute;
     font-size: 12px;
     top:5px;
     left:35px;
     color:white;
     z-index: 2;
     color:#B7FF00;
     font-weight: bold;
   }

   .down{
     position:absolute;
     font-size: 12px;
     top:23px;
     left:35px;
     color:white;
     z-index: 2;
     font-weight: bold;
   }

   .fa-sort-up{
     position:absolute;
     font-size: 40px;
     top:1px;
     left:5px;
     opacity: 0.5;
     color:grey;
     z-index: 1;
   }

   .fa-sort-down{
     position:absolute;
